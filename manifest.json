{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persittent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.4.1.min.js",
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "food.png",
        "default_popup": "popup.html"
    }
}